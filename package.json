{
  "name": "aptos-blueprint",
  "description": "a code generation tool for seamless interaction with Aptos smart contracts",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "author": "aptoslabs.com",
  "keywords": [
    "aptos",
    "blueprint",
    "abi gen",
    "code generation",
    "code generator",
    "code-gen",
    "codegen"
  ],
  "bin": {
    "aptos-blueprint": "./dist/index.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "dev": "npm run build && node dist/index.js",
    "gen": "npm run build && node dist/index.js",
    "jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "dev-esm": "ts-node --esm index.ts",
    "fast": "tsx index.ts",
    "publish": "npm run build && node dist/tests/publish.js",
    "_fmt": "prettier 'generated/**/*.ts' 'src/*.ts' 'src/**/*.ts' 'tests/*.ts' 'tests/**/*.ts'",
    "fmt": "pnpm _fmt --write",
    "lint": "pnpm eslint 'src/**/*.ts' 'tests/*.ts' 'tests/**/*.ts'",
    "prepublish": "npm run build && npm publish"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.13.0",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/pako": "^2.0.3",
    "@types/prompts": "^2.4.9",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "kolorist": "^1.8.0",
    "pako": "^2.1.0",
    "prompts": "^2.4.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aptos-labs/blueprint.git"
  },
  "homepage": "https://github.com/aptos-labs/blueprint/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/aptos-labs/blueprint/issues"
  }
}
